{
  "name": "matchee",
  "version": "0.4.3",
  "description": "Type-safe expression matching",
  "keywords": [
    "match",
    "pattern matching",
    "type-safe",
    "typescript"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest",
    "lint": "eslint src --ext .ts",
    "release": "npx changeset version && npx changeset publish"
  },
  "author": "Gabriel Jacinto",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabrieljmj/matchee.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@total-typescript/ts-reset": "^0.4.2",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.7",
    "tsup": "^6.7.0",
    "typescript": "^4.9.5",
    "vitest": "^0.30.0"
  }
}
